<% unless sbj_block.blank? %>
<strong><%= sbj_block.first[3] %></strong>
<div class="row">
    <div class="col-lg-4 col-xs-4">
        <small>Opportunity Name</small>
    </div>
    <div class="col-lg-4 col-xs-4">
        <small>Belongs to Account Name</small>
    </div>
    <div class="col-lg-4 col-xs-4">
        <small>Created at DateTime</small>
    </div>
</div>
<% sbj_block.each do |opaprv| %>
<div class="row">
    <div class="col-lg-4 col-xs-4">
        <%= link_to opaprv[1], opportunity_path(opaprv[0]) %>
    </div>
    <div class="col-lg-4 col-xs-4">
        <%#= link_to opaprv[4], account_path(opaprv[5]) %>
    </div>
    <div class="col-lg-4 col-xs-4">
        <%= opaprv[2].strftime('%d %b %Y %H:%M:%S')%>
    </div>
</div>
<% end %>
<!-- loop end  -->
<br>
<%= link_to 'See more', opportunities_path %>
<%#= link_to 'See more', case_path %>
<%#= link_to 'See more', marketing_path %>
<hr>
<% end %>
<!-- unless sbj_open end -->
