<div class="row">
  <div class="col-lg-1">
    <%= account.id %>
  </div>
  <div class="col-lg-9">
    <%= account.name %>
  </div>
  <div class="col-lg-2">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
      <i class="caret"></i>
    </a>
    <ul class="dropdown-menu">
      <li><%= link_to 'Show', account_path(account) %></li>
      <li><%= link_to 'Edit', '#', data: {toggle: "modal", target: "#accountModalForm"} %></li>
      <li><%= link_to 'Delete', account_path(account), method: :delete,
      data: {confirm: "Are you sure?"} %></li>
    </ul>
  </div>
</div>
