<div class="tab-pane fade in" id="opportunityDocs">
  <h4>Opportunity Related Documents</h4>

  <div class="panel panel-default panel-body">
    <%= simple_form_for :doc do |f| %>
    <%= f.label "Upload Documents" %>
    <%= file_field_tag "pics_doc[]",type: :image, multiple: true %>
    <% end %>
  </div>

  <div class="row">
        <% for i in 1..6 do %>
        <div class="col-lg-3">
            <br>
            <%= image_tag('http://placehold.it/200x250', class: "img-responsive img-center")%>
        </div>
        <% end %>
      </div>
</div>
