<div class="tab-pane fade in" id="accountContacts">
    <h4>Account Related Contacts</h4>

    <%= link_to 'New Contact', '#', data: {toggle: "modal", target: "#contactModalForm"},
    class: "btn btn-default" %>
    <%= render 'contacts/form' %>

    <div class="row">
    <% for i in 1..6 do %>
    <div class="col-lg-6">
        <br>
        <div class="media">
            <div class="media-left">
                <%= image_tag('http://placehold.it/35x35', class: "media-object") %>
            </div>
            <div class="media-body">
                <h5 class="media-heading">
                    Mohammad<br>
                    <small>Title</small>
                </h5>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                Any important info here
            </div>
        </div>
    </div>
    <% end %>
  </div>
</div>
